	follows. -->
<Events>
	<Event type="net.vicp.madz.infra.service.event.ApplicationServiceEvent">
		<Handlers>
			<Handler
				type="net.vicp.madz.infra.service.handler.ApplicationServiceEventHandler" />
		</Handlers>
	</Event>
	<Event
		type="net.vicp.madz.infra.security.messaging.event.outbound.TenantLogEvent">
		<Handlers>
			<Handler
				type="net.vicp.madz.infra.security.messaging.handler.TenantLogEventHandler" />
		</Handlers>
	</Event>
</Events>
