<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_5-0.dtd">
<sun-web-app error-url="">
	<context-root>/JShop</context-root>
	<security-role-mapping>
		<role-name>ADMIN</role-name>
		<principal-name>administrator</principal-name>
		<group-name>ADMINGroup</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>OP</role-name>
		<group-name>OPGroup</group-name>
	</security-role-mapping>
	<security-role-mapping>
		<role-name>SA</role-name>
		<principal-name>administrator</principal-name>
	</security-role-mapping>
	<ejb-ref>
		<ejb-ref-name>CommonObjectQueryFacadeBean</ejb-ref-name>
		<jndi-name>CommonObjectQueryFacadeBean</jndi-name>
	</ejb-ref>
	<class-loader delegate="true" />
	<jsp-config>
		<property name="keepgenerated" value="true">
			<description>Keep a copy of the generated servlet class' java code.</description>
		</property>
	</jsp-config>
	<property name="valve_1" value="flex.messaging.security.TomcatValve" />
</sun-web-app>
